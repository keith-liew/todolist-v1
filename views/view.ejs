<%- include("header") -%>
    <div class="box" id="heading">
        <h1>It's <%= title %>
        </h1>

    </div>

    <div class="box">
        <% items.map(function(item){ %>
            <div class="item">
                <input type="checkbox" />
                <p>
                    <%=item%>
                </p>
            </div>
            <%}) %>

                <form class="item " action="/" method="post">
                    <input type="text" name="newItem" placeholder="Things needed to be done" autocomplete="off">
                    <button type="submit" name="list" value=<%=title %>>+</button>
                </form>
    </div>

<%- include("footer") -%>